ngx_addon_name=appguard_nginx_module

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=appguard_nginx_module
    ngx_module_srcs="$ngx_addon_dir/appguard_nginx_module.c"

    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES appguard_nginx_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/appguard_nginx_module.c"
fi